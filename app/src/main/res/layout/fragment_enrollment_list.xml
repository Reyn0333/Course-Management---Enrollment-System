<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.registrardashboard.enrollmentList.EnrollmentList">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10sp"
        android:layout_marginTop="100dp"
        android:text="Period"
        android:textSize="15sp" />


    <EditText
        android:id="@+id/editText"
        android:layout_width="121dp"
        android:layout_height="40dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/custom_edit_text"
        android:hint="Enter text"
        android:padding="10dp"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10sp"
        android:layout_marginTop="150sp"
        android:text="Course"
        android:textSize="15sp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="121dp"
        android:layout_height="40dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="140dp"
        android:background="@drawable/custom_edit_text"
        android:hint="Enter text"
        android:padding="10dp"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10sp"
        android:layout_marginTop="190sp"
        android:text="Section"
        android:textSize="15sp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="121dp"
        android:layout_height="40dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="190dp"
        android:background="@drawable/custom_edit_text"
        android:hint="Enter text"
        android:padding="10dp"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="190sp"
        android:layout_marginTop="100dp"
        android:text="Department"
        android:textSize="15sp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="121dp"
        android:layout_height="40dp"
        android:layout_marginStart="280dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/custom_edit_text"
        android:hint="Enter text"
        android:padding="10dp"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="220sp"
        android:layout_marginTop="150sp"
        android:text="Year"
        android:textSize="15sp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="121dp"
        android:layout_height="40dp"
        android:layout_marginStart="280dp"
        android:layout_marginTop="140dp"
        android:background="@drawable/custom_edit_text"
        android:hint="Enter text"
        android:padding="10dp"
        android:textColor="#000000" />


    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="412dp"
        android:layout_marginTop="300dp">>



        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="16dp">

            <!-- Table Layout -->
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="*"
                android:paddingTop="15dp">

                <!-- Table Headers -->
                <TableRow>
                    <TextView android:text="No." android:padding="7dp" android:background="@color/lavander" android:textStyle="bold"  />
                    <TextView android:text="Code" android:padding="7dp" android:background="@color/lavander" android:textStyle="bold" />
                    <TextView android:text="Name" android:padding="7dp" android:background="@color/lavander" android:textStyle="bold"/>
                    <TextView android:text="Gender" android:padding="7dp" android:background="@color/lavander" android:textStyle="bold"/>
                    <TextView android:text="Course" android:padding="7dp" android:background="@color/lavander" android:textStyle="bold"/>
                    <TextView android:text="Year" android:padding="7dp" android:background="@color/lavander" android:textStyle="bold"/>
                    <TextView android:text="Units" android:padding="7dp" android:background="@color/lavander" android:textStyle="bold" />
                    <TextView android:text="Section" android:padding="7dp" android:background="@color/lavander" android:textStyle="bold"/>
                    <TextView android:text="Contact No." android:padding="7dp" android:background="@color/lavander" android:textStyle="bold"/>
                    <TextView android:text="Birth Date" android:padding="7dp" android:background="@color/lavander" android:textStyle="bold"/>
                    <TextView android:text="Address" android:padding="7dp" android:background="@color/lavander" android:textStyle="bold"/>
                </TableRow>


                <TableRow>
                    <TextView android:text="-." android:padding="7dp" android:background="@color/white" android:textStyle="bold"  />
                    <TextView android:text="-" android:padding="7dp" android:background="@color/white" android:textStyle="bold" />
                    <TextView android:text="-" android:padding="7dp" android:background="@color/white" android:textStyle="bold"/>
                    <TextView android:text="-" android:padding="7dp" android:background="@color/white" android:textStyle="bold"/>
                    <TextView android:text="-" android:padding="7dp" android:background="@color/white" android:textStyle="bold"/>
                    <TextView android:text="-" android:padding="7dp" android:background="@color/white" android:textStyle="bold"/>
                    <TextView android:text="-" android:padding="7dp" android:background="@color/white" android:textStyle="bold" />
                    <TextView android:text="-" android:padding="7dp" android:background="@color/white" android:textStyle="bold"/>
                    <TextView android:text="- " android:padding="7dp" android:background="@color/white" android:textStyle="bold"/>
                    <TextView android:text="-" android:padding="7dp" android:background="@color/white" android:textStyle="bold"/>
                    <TextView android:text="-" android:padding="7dp" android:background="@color/white" android:textStyle="bold"/>
                </TableRow>

            </TableLayout>

        </LinearLayout>




    </HorizontalScrollView>

</FrameLayout>
